{
	"name": "Data-Ingestion",
	"properties": {
		"activities": [
			{
				"name": "Postgress-to-adls Data Ingestion",
				"type": "ExecutePipeline",
				"dependsOn": [
					{
						"activity": "SQLServer-to-adls Data Ingestion",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"pipeline": {
						"referenceName": "Postgress-to-adls",
						"type": "PipelineReference"
					},
					"waitOnCompletion": true
				}
			},
			{
				"name": "SQLServer-to-adls Data Ingestion",
				"type": "ExecutePipeline",
				"state": "Inactive",
				"onInactiveMarkAs": "Succeeded",
				"dependsOn": [],
				"policy": {
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"pipeline": {
						"referenceName": "SQLServer-to-adls",
						"type": "PipelineReference"
					},
					"waitOnCompletion": true
				}
			},
			{
				"name": "ErrorHandling",
				"type": "ExecutePipeline",
				"dependsOn": [
					{
						"activity": "Postgress-to-adls Data Ingestion",
						"dependencyConditions": [
							"Failed",
							"Skipped"
						]
					}
				],
				"policy": {
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"pipeline": {
						"referenceName": "GenericErrorHandling",
						"type": "PipelineReference"
					},
					"waitOnCompletion": true,
					"parameters": {
						"receiver": {
							"value": "@pipeline().parameters.receiver",
							"type": "Expression"
						},
						"Error": {
							"value": "@concat('|',activity('SQLServer-to-adls Data Ingestion').error?.message,'|',activity('Postgress-to-adls Data Ingestion').error?.message,'|')",
							"type": "Expression"
						}
					}
				}
			}
		],
		"parameters": {
			"receiver": {
				"type": "string",
				"defaultValue": "yashkothari5499@gmail.com"
			}
		},
		"annotations": []
	}
}