{
	"name": "ADLS-to-SQL",
	"properties": {
		"activities": [
			{
				"name": "ADLS-to-SQLSerevr",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ParquetSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFolderPath": "tables/silver_flights",
							"wildcardFileName": "*parquet",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "ParquetReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "id",
									"type": "Int64",
									"physicalType": "INT64"
								},
								"sink": {
									"name": "id",
									"type": "Int64",
									"physicalType": "bigint"
								}
							},
							{
								"source": {
									"name": "flight_code",
									"type": "String",
									"physicalType": "UTF8"
								},
								"sink": {
									"name": "flight_code",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"sink": {
									"name": "flight_date",
									"type": "DateTime",
									"physicalType": "date"
								}
							},
							{
								"source": {
									"name": "source_airport",
									"type": "String",
									"physicalType": "UTF8"
								},
								"sink": {
									"name": "source_airport",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "destination_airport",
									"type": "String",
									"physicalType": "UTF8"
								},
								"sink": {
									"name": "destination_airport",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "source_city",
									"type": "String",
									"physicalType": "UTF8"
								},
								"sink": {
									"name": "source_city",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "destination_city",
									"type": "String",
									"physicalType": "UTF8"
								},
								"sink": {
									"name": "destination_city",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "estimated_departure_time",
									"type": "DateTime",
									"physicalType": "INT96"
								},
								"sink": {
									"name": "estimated_departure_time",
									"type": "DateTime",
									"physicalType": "datetime"
								}
							},
							{
								"source": {
									"name": "departure_timestamp",
									"type": "DateTime",
									"physicalType": "INT96"
								},
								"sink": {
									"name": "departure_timestamp",
									"type": "DateTime",
									"physicalType": "datetime"
								}
							},
							{
								"source": {
									"name": "estimated_arrival_time",
									"type": "DateTime",
									"physicalType": "INT96"
								},
								"sink": {
									"name": "estimated_arrival_time",
									"type": "DateTime",
									"physicalType": "datetime"
								}
							},
							{
								"source": {
									"name": "arrival_timestamp",
									"type": "DateTime",
									"physicalType": "INT96"
								},
								"sink": {
									"name": "arrival_timestamp",
									"type": "DateTime",
									"physicalType": "datetime"
								}
							},
							{
								"source": {
									"name": "total_passengers",
									"type": "Int64",
									"physicalType": "INT64"
								},
								"sink": {
									"name": "total_passengers",
									"type": "Int32",
									"physicalType": "int"
								}
							},
							{
								"source": {
									"name": "status",
									"type": "String",
									"physicalType": "UTF8"
								},
								"sink": {
									"name": "status",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"sink": {
									"name": "run_id",
									"type": "Int32",
									"physicalType": "int"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ADLS_Databricks_parquet_DS",
						"type": "DatasetReference",
						"parameters": {
							"adls_url": {
								"value": "@pipeline().parameters.adls_url",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "SQLDatabase_AutoIR_DS",
						"type": "DatasetReference",
						"parameters": {
							"Sql_connectionstring": {
								"value": "@pipeline().parameters.Sql_connectionstring",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"adls_url": {
				"type": "string"
			},
			"Sql_connectionstring": {
				"type": "string"
			}
		},
		"annotations": []
	}
}